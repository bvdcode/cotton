<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UserSecretsId>0944ae36-b540-443b-bda8-1974c0e216ff</UserSecretsId>
		<DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
		<SpaRoot>..\cotton.client</SpaRoot>
		<SpaProxyLaunchCommand>npm run dev</SpaProxyLaunchCommand>
		<SpaProxyServerUrl>https://localhost:51856</SpaProxyServerUrl>
		<PackageLicenseExpression>AGPL-3.0-only</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.SpaProxy">
			<Version>10.0.0</Version>
		</PackageReference>
		<ProjectReference Include="..\cotton.client\cotton.client.esproj">
			<ReferenceOutputAssembly>false</ReferenceOutputAssembly>
		</ProjectReference>
		<PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="10.0.0">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Mapster" Version="7.4.0" />
		<ProjectReference Include="..\Cotton.Shared\Cotton.Shared.csproj" />
		<ProjectReference Include="..\Cotton.Storage\Cotton.Storage.csproj" />
		<PackageReference Include="EasyExtensions.Mediator" Version="3.0.0" />
		<ProjectReference Include="..\Cotton.Topology\Cotton.Topology.csproj" />
		<ProjectReference Include="..\Cotton.Database\Cotton.Database.csproj" />
		<ProjectReference Include="..\Cotton.Autoconfig\Cotton.Autoconfig.csproj" />
		<ProjectReference Include="..\Cotton.Validators\Cotton.Validators.csproj" />
		<PackageReference Include="EasyExtensions.EntityFrameworkCore" Version="3.0.0" />
		<PackageReference Include="EasyExtensions.AspNetCore.Authorization" Version="3.0.0" />
		<PackageReference Include="EasyExtensions.EntityFrameworkCore.Npgsql" Version="3.0.0" />
		<PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.22.1" />
		<Folder Include="wwwroot\" />
	</ItemGroup>

</Project>
